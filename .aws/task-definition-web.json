{
    "family": "tartan-ctf-web-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "web-container",
            "image": "tartan-ctf-web",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/tartan-ctf-web",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "environment": [
                {
                    "name": "PORT",
                    "value": "3000"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::946926531275:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::946926531275:role/ecsTaskExecutionRole"
}